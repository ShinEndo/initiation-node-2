<h1>Subscriber Data for <%= subscriber.fullname %></h1>
<table>
  <tr>
    <th>Name</th>
    <td><%= subscriber.name %></td>
  </tr>
  <tr>
    <th>Email</th>
    <td><%= subscriber.email %></td>
  </tr>
  <tr>
    <th>Zip Code</th>
    <td><%= subscriber.zipCode %></td>
  </tr>
</table>

<div>
  <a href="/subscribers">View all</a>
</div>
<div>
  <a href="<%= `/subscribers/${subscriber._id}/edit` %>"
    >Edit Subscriber Details</a
  >
</div>
<div>
  <a
    href="<%= `subscribers/${subscriber._id}/delete?_method=DELETE` %>"
    onclick="return confirm('Are you want to delete this record?')"
    >DELETE</a
  >
</div>
